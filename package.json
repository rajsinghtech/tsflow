{
  "name": "tsflow",
  "version": "1.0.0",
  "description": "Tailscale Network Flow Visualizer - A modern web application for visualizing and analyzing network traffic flows within Tailscale networks",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
