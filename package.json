    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "lucide-react": "^0.323.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
